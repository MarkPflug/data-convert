<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PublishAot>true</PublishAot>
		<InvariantGlobalization>true</InvariantGlobalization>
		<OptimizationPreference>Size</OptimizationPreference>
		<IlcGenerateMstatFile>true</IlcGenerateMstatFile>
		<IlcGenerateDgmlFile>true</IlcGenerateDgmlFile>
		<XmlResolverIsNetworkingEnabledByDefault>false</XmlResolverIsNetworkingEnabledByDefault>
	</PropertyGroup>

	<ItemGroup>
		<InternalsVisibleTo Include="Test" />
		<PackageReference Include="PublishAotCompressed" Version="1.0.5" />
		<PackageReference Include="Parquet.Net" Version="5.1.1" />
		<PackageReference Include="Sylvan.Data" Version="0.2.16" />
		<PackageReference Include="Sylvan.Data.Csv" Version="1.4.2" />
		<PackageReference Include="Sylvan.Data.Excel" Version="0.4.27" />
		<PackageReference Include="Sylvan.Data.XBase" Version="0.1.3" />
	</ItemGroup>

</Project>
